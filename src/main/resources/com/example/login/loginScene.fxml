<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" prefHeight="533.0" prefWidth="556.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.login.Controllers.LoginSceneController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <GridPane prefHeight="198.0" prefWidth="516.0">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
        <Label alignment="CENTER" prefHeight="52.0" prefWidth="246.0" text="Username:">
           <font>
               <Font size="24.0" />
           </font>
        </Label>
        <Label alignment="CENTER" prefHeight="52.0" prefWidth="246.0" text="Password:" GridPane.rowIndex="1">
           <font>
               <Font size="24.0" />
           </font>
        </Label>
        <TextField fx:id="userName" prefHeight="51.0" prefWidth="258.0" GridPane.columnIndex="1" />
        <PasswordField fx:id="password" prefHeight="52.0" prefWidth="258.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
    </GridPane>
   <BorderPane prefHeight="183.0" prefWidth="516.0">
      <center>
         <Label fx:id="loginText" prefHeight="60.0" prefWidth="491.0" textFill="RED" BorderPane.alignment="CENTER">
            <font>
               <Font size="27.0" />
            </font>
         </Label>
      </center>
      <top>
         <Button fx:id="buttonScene" alignment="CENTER" contentDisplay="CENTER" defaultButton="true" mnemonicParsing="false" onAction="#loginAttempt" prefHeight="53.0" prefWidth="179.0" snapToPixel="false" text="Login" textAlignment="CENTER" BorderPane.alignment="CENTER">
            <font>
               <Font size="24.0" />
            </font></Button>
      </top>
   </BorderPane>
</VBox>
