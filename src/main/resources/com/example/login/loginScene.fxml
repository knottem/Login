<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="533.0" prefWidth="556.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.login.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <Pane prefHeight="800.0" prefWidth="600.0">
      <children>
         <ProgressIndicator fx:id="progressIndicator" layoutX="245.0" layoutY="65.0" progress="0.0" />
         <BorderPane layoutX="127.0" layoutY="134.0" prefHeight="0.0" prefWidth="276.0">
            <right>
               <Button fx:id="testButton" mnemonicParsing="false" onAction="#onTestButtonClick" text="Test" BorderPane.alignment="CENTER" />
            </right>
            <left>

                <Label fx:id="welcomeText" visible="false" BorderPane.alignment="CENTER" />
            </left>
         </BorderPane>
          <Button fx:id="helloButton" layoutX="241.0" layoutY="233.0" onAction="#onHelloButtonClick" text="Hello!" />
         <Button fx:id="progressButton" layoutX="214.0" layoutY="340.0" mnemonicParsing="false" onAction="#onProgressButtonClick" text="Progress 10%" />
         <Button fx:id="buttonScene" layoutX="232.0" layoutY="443.0" mnemonicParsing="false" onAction="#changeScene" text="Change Scene" />
      </children>
   </Pane>
</VBox>
